    <script>

        var fullimg1=docoment.getElementByid("fullimg1");
        var fullimg2=docoment.getElementByid("fullimg2");

        function openfullimg2(pic){
            fullimg1.style.disply = "flex";
            fullimg1.src = pic;
        }
        function closefullimg2(){
            fullimg1.style.disply = "none";
        }

